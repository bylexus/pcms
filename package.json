{
    "name": "pcms-doc",
    "version": "0.2.0",
    "description": "Documentation page for pcms",
    "main": "server.js",
    "scripts": {
        "css-watch": "sass --watch themes/pcms-doc/sass/main.scss:themes/pcms-doc/static/css/main.css",
        "build-css": "NODE_ENV=production sass -s compressed themes/pcms-doc/sass/main.scss:themes/pcms-doc/static/css/main.css",
        "dev-server": "NODE_ENV=development DEBUG=server,pcms npx nodemon -w server.js -w site/ -w themes/pcms-doc/ server.js"
    },
    "author": "Alexander Schenkel <alex@alexi.ch>",
    "license": "MIT",
    "dependencies": {
        "pcms": "^0.2.0"
    },
    "devDependencies": {
        "nodemon": "^1.18.10",
        "sass": "^1.17.3"
    }
}
